[HDP-{{ hdp_version.split('-')[0] }}-GPL]
name=HDP-GPL Version - HDP-GPL-{{ hdp_version }}
baseurl={{ repo_base_url }}/hdp/HDP-GPL/centos{{ ansible_distribution_major_version }}/{{ hdp_version }}
gpgcheck=1
gpgkey={{ repo_base_url }}/hdp/HDP-GPL/centos{{ ansible_distribution_major_version }}/{{ hdp_version }}/RPM-GPG-KEY/RPM-GPG-KEY-Jenkins
enabled=1
priority=1